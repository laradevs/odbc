    ],
    "autoload": {
        "psr-4": {
            "LaraDevs\\Odbc\\": "src/"
        }
    },
    "description": "It is a component for connect ODBC with laravel.",
    "extra": {
        "laravel": {
            "providers": [
                "LaraDevs\\Odbc\\OdbcServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "laradevs/odbc",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^5.8|6.*|7.*",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "LaraDevs\\Odbc\\Test\\": "tests/"
        }
    }
}
